{
    "family": "mh-1-listen", 
    "networkMode": "awsvpc", 
    "executionRoleArn": "arn:aws:iam::519550959950:role/ecsTaskExecutionRole",
    "containerDefinitions": [
        {
            "name": "mh-1-container", 
            "image": "httpd:2.4", 
            "portMappings": [
                {
                    "containerPort": 80, 
                    "hostPort": 80, 
                    "protocol": "tcp"
                }
            ], 
            "essential": true, 
            "entryPoint": [
                "sh",
                "-c"
            ], 
            "command": [
                "/bin/sh -c \"echo 'hello world'\""
            ],
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "mh-1",
                    "awslogs-region": "us-east-1",
                    "awslogs-stream-prefix": "mh-1"
                }
            }
        }
    ], 
    "requiresCompatibilities": [
        "FARGATE"
    ], 
    "cpu": "256", 
    "memory": "512"
}